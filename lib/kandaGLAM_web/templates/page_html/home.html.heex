<.flash_group flash={@flash} />
<section class="relative">
  <div class="flex items-center justify-center mb-12 relative z-5">
    <img src={~p"/images/kandaglam.png"} alt="" class="h-full w-32" />
  </div>
  <div>
    <img src={~p"/images/default_bg.webp"} class="absolute w-full object-cover z-1 top-0" />
  </div>
  <div class="z-5">
    <div class="search-tab">
      <div
        id="tab-quick"
        class="search-tab-item active-tab-item"
        phx-click={set_active_tab("#tab-quick") |> show_active_tab("#content_tab_quick")}
      >
        <p>Search</p>
      </div>
      <div
        id="tab-gallery"
        class="search-tab-item"
        phx-click={set_active_tab("#tab-gallery") |> show_active_tab("#content_tab_gallery")}
      >
        <p>Gallery</p>
      </div>
      <div
        id="tab-library"
        class="search-tab-item"
        phx-click={set_active_tab("#tab-library") |> show_active_tab("#content_tab_library")}
      >
        <p>Library</p>
      </div>
      <div
        id="tab-archive"
        class="search-tab-item"
        phx-click={set_active_tab("#tab-archive") |> show_active_tab("#content_tab_archive")}
      >
        <p>Archive</p>
      </div>
      <div
        id="tab-museum"
        class="search-tab-item"
        phx-click={set_active_tab("#tab-museum") |> show_active_tab("#content_tab_museum")}
      >
        <p>Museum</p>
      </div>
    </div>
    <div class="bg-orange-200 p-5 rounded-bl-lg rounded-br-lg">
      <div id="content_tab_quick" class="tab-content">
        <input type="search" class="w-full p-2 rounded-md" placeholder="Quick Search" />
      </div>
      <div id="content_tab_gallery" class="tab-content hidden">
        <input type="search" class="w-full p-2 rounded-md" placeholder="Gallery Search" />
      </div>
      <div id="content_tab_library" class="tab-content hidden">
        <input type="search" class="w-full p-2 rounded-md" placeholder="Library Search" />
      </div>
      <div id="content_tab_archive" class="tab-content hidden">
        <input type="search" class="w-full p-2 rounded-md" placeholder="Archive Search" />
      </div>
      <div id="content_tab_museum" class="tab-content hidden">
        <input type="search" class="w-full p-2 rounded-md" placeholder="Museum Search" />
      </div>
    </div>
  </div>
  <.greet name="Kandaga" />
